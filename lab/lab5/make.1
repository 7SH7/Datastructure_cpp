sortDriver: sortDriver.o comparator.o printlist.o bubble.o quick.o insertion.o merge.o selection.o
	g++ sortDriver.o comparator.o printlist.o bubble.o quick.o insertion.o merge.o selection.o\
		-I../../include -L../../lib -lnowic -o sortDriver
		
sortDriver.o: sortDriver.cpp
	g++ -c -std=c++11 -Wall sortDriver.cpp -I../../include 
comparator.o: comparator.cpp
	g++ -c -std=c++11 -Wall comparator.cpp 
printlist.o: printlist.cpp
	g++ -c -std=c++11 -Wall printlist.cpp 
bubble.o: bubble.cpp
	g++ -c -std=c++11 -Wall bubble.cpp -I../../include
insertion.o: insertion.cpp
	g++ -c -std=c++11 -Wall insertion.cpp -I../../include
merge.o: merge.cpp
	g++ -c -std=c++11 -Wall merge.cpp -I../../include
quick.o: quick.cpp
	g++ -c -std=c++11 -Wall quick.cpp -I../../include
selection.o: selection.cpp
	g++ -c -std=c++11 -Wall selection.cpp -I../../include

clean:
	rm -f *.o sortDriver.exe sortDriver
